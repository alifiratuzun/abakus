{% extends "layout.njk" %}

{% block content %}
  <article class="project">
    <h1>{{ title }}</h1>
    <p><em>{{ location }}, {{ year }}</em></p>
    {% for img in images %}
      <img src="{{ img }}" alt="{{ title }}">
    {% endfor %}
    <div class="body">
      {{ content | safe }}
    </div>
  </article>
{% endblock %}
